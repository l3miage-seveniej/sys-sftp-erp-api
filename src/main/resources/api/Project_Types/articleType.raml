#%RAML 1.0 DataType

type: object
properties:
  id:
    type: string
    required: true 
  title:
    type:  string
    required: false
  type_article:
    required: false
    enum:
      - Fourniture
      - Mobilier
      - Appareil
  stock_available: 
    type: integer
    required: true
  data_update: 
    type: datetime
    default: 1900-01-01T00:00:00Z
    required : false
  description:
    type: string
    required: false
     

    


